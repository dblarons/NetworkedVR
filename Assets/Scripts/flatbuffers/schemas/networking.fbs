namespace NetworkingFBS;

struct Position {
  x:float;
  y:float;
  z:float;
}

struct Rotation {
  x:float;
  y:float;
  z:float;
  w:float;
}

table ObjectState {
  guid:string;
  prefabId:int;
  position:Position;
  rotation:Rotation;
}

table WorldUpdate {
  primaries:[ObjectState];
  secondaries:[ObjectState];
}

root_type WorldUpdate;
